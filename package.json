  "author": "Abdalla Arbab",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "laravel-mix": "^2.1",
    "postcss-import": "^12.0.0",
    "tailwindcss": "^0.6.6",
    "vue": "^2.5.17",
    "vue-good-table": "^2.14.6",
    "vue-notification": "^1.3.13",
    "vue-slide-up-down": "^1.3.3",
    "vue-slideout": "^1.7.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
